{
  "name": "dlamedica-webapp",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "prepare": "husky install",
    "seed:jobs": "tsx scripts/seed-job-offers.ts",
    "seed:accounts": "NODE_ENV=development tsx scripts/create-test-accounts.ts",
    "seed:all": "NODE_ENV=development tsx scripts/seed-all-data.ts",
    "seed:calculators": "NODE_ENV=development tsx scripts/seed-calculators-complete.ts",
    "seed:master": "NODE_ENV=development tsx scripts/seed-master.ts",
    "seed:test-accounts": "node scripts/seed-test-accounts.js"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/leaflet": "^1.9.20",
    "@types/pg": "^8.15.5",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.10.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.25",
    "fuse.js": "^7.1.0",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^3.0.1",
    "leaflet": "^1.9.4",
    "lottie-web": "^5.13.0",
    "lucide-react": "^0.533.0",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "prisma": "^6.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^16.5.2",
    "react-icons": "^5.5.0",
    "react-image-crop": "^11.0.10",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^3.1.0",
    "react-router-dom": "^7.8.0",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
